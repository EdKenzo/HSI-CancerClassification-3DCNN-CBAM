    "import matplotlib.pyplot as plt\n",
    "import os\n",
    "import scipy.io \n",
    "import numpy as np\n",
    "import cv2\n",
    "import random\n",
    "from sklearn.decomposition import PCA\n",
    "from sklearn.utils import shuffle\n",
    "from scipy.signal import savgol_filter\n",
    "import pandas as pd,
    "from PIL import Image\n",
    "import random\n",
    "from collections import Counter"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Patch Extraction for Train Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Find annotated pixels\n",
    "def find_colour_positions(img,colour):\n",
    "    return set(zip(*np.where(\n",
    "        (img[:, :, 0] == colour[0]) & \n",
    "        (img[:, :, 1] == colour[1]) &\n",
    "        (img[:, :, 2] == colour[2])\n",
    "    )))\n",
    "    \n",
    "\n",
    "# PCA denoising function\n",
    "def denoise_pca(spectra, n_components=80):\n",
    "    pca = PCA(n_components=n_components)\n",
    "    clean_spectra = pca.fit_transform(spectra)\n",
    "    return pca.inverse_transform(clean_spectra)\n",
    "\n",
    "def preprocess_spectra(spectra, wavenumbers):\n",
    "    # Step 1: Amide I region quality filter\n",
    "    amide_band = np.where((wavenumbers >= 1600) & (wavenumbers <= 1700))[0]\n",
    "    amide_max = np.max(spectra[:, amide_band], axis=1)\n",
    "    valid = (amide_max >= 0.1) & (amide_max <= 2)\n",
    "    spectra = spectra[valid]\n",
    "\n",
    "    # Step 2: Remove wax regions\n",
    "    wax_range = (\n",
    "        ((wavenumbers >= 1000) & (wavenumbers <= 1319)) | \n",
    "        ((wavenumbers >= 1481) & (wavenumbers <= 1769)) |\n",
    "        ((wavenumbers >= 2986) & (wavenumbers <= 3569))\n",
    "    )\n",
    "    spectra = spectra[:, wax_range]\n",
    "    wavenumbers = wavenumbers[wax_range]\n",
    "\n",
    "    # Step 3: SavGol first derivative\n",
    "    spectra = savgol_filter(spectra, 19, 4, deriv=1, axis=1)\n",
    "\n",
    "    # Step 4: Refine wavenumber range again after deriv\n",
    "    refined_range = (\n",
    "        ((wavenumbers >= 1019) & (wavenumbers <= 1300)) |\n",
    "        ((wavenumbers >= 1500) & (wavenumbers <= 1750)) |\n",
    "        ((wavenumbers >= 3005) & (wavenumbers <= 3550))\n",
    "    )\n",
    "    spectra = spectra[:, refined_range]\n",
    "    wavenumbers = wavenumbers[refined_range]\n",
    "\n",
    "    return spectra, wavenumbers\n",
    "\n",
    "\n",
    "# Define file locations - change to your own\n",
    "data_location = r\"C:\\location\\to\\NumPy\\DataCube\"\n",
    "annotation_location = r\"C:\\location\\to\\annotated_image\\overlay\"\n",
    "\n",
    "core_names = [\n",
    "    \"A12\", \"C10\", \"C16\", \"C5\", \"C8\", \"D5\", \"D7\", \"D8\", \"D9\", \"E7\", \"E8\", \"F5\", \"F7\", \"F8\",\n",
    "    \"G6\", \"H7\", \"H10\", \"H9\", \"I3\", \"I5\", \"A14\", \"I7\", \"B9\", \"I10\", \"K1\", \"K10\", \"K6\", \"K9\", \n",
    "    \"B11\", \"L10\", \"L2\", \"L3\", \"M1\", \"M3\", \"M4\", \"M6\", \"M9\", \"M10\", \"M11\", \"M13\"\n",
    "]\n",
    "\n",
    "annotation_colours = {\n",
    "    \"red\": (255, 0, 0),\n",
    "    \"green\": (103, 193, 66),\n",
    "    \"orange\":(243,143,53),\n",
    "    \"purple\":(165,55,156)\n",
    "}\n",
    "\n",
    "all_spectra = []\n",
    "count_spectra = []\n",
    "spectral_patches = []\n",
    "chem_img_patches = []\n",
    "labels = []\n",
    "core_id = []\n",
    "\n",
    "PATCH_SIZE = 7\n",
    "half_patch = PATCH_SIZE // 2\n",
    "\n",
    "\n",
    "# Process each core\n",
    "for core_name in core_names:\n",
    "    mat_path = os.path.join(data_location, core_name + \".npz\")\n",
    "    overlay_img_path = os.path.join(annotation_location, core_name + \".png\")\n",
    "\n",
    "    data = np.load(mat_path)\n",
    "\n",
    "    spectra = data[\"spectra\"]\n",
    "    wavenumbers = data[\"wavenumbers\"].flatten()\n",
    "    indices = data[\"indices\"].flatten()\n",
    "    \n",
    "    # Annotated image to extract pixel locations\n",
    "    annotated_img = np.array(Image.open(overlay_img_path))\n",
    "    \n",
    "    # Fix dimensions (Swapped to match MATLAB)\n",
    "    ypixels, xpixels = annotated_img.shape[:2]\n",
    "\n",
    "    # Convert indices to (row, col) positions for NumPy format\n",
    "    rows = indices % ypixels  \n",
    "    cols = indices // ypixels\n",
    "\n",
    "    # Convert to NumPy array for consistency\n",
    "    pixel_positions = np.column_stack((rows, cols))  \n",
    "\n",
    "    # set of (row,col) for each colour\n",
    "    malignant_positions = find_colour_positions(annotated_img, annotation_colours[\"red\"]) | find_colour_positions(annotated_img, annotation_colours[\"purple\"])\n",
    "    normal_positions = find_colour_positions(annotated_img, annotation_colours[\"green\"]) | find_colour_positions(annotated_img, annotation_colours[\"orange\"])\n",
    "\n",
    "    # Extract spectra and indices for annotated regions\n",
    "    core_spectra = []\n",
    "    core_indices = []\n",
    "    label = []\n",
    "    \n",
    "    for i, (row, col) in enumerate(pixel_positions):\n",
    "        if (row, col) in malignant_positions:\n",
    "            core_spectra.append(spectra[i])\n",
    "            label.append(1)\n",
    "            core_indices.append((row, col))\n",
    "        elif (row, col) in normal_positions:\n",
    "            core_spectra.append(spectra[i])\n",
    "            label.append(0)\n",
    "            core_indices.append((row, col))\n",
    "\n",
    "    core_spectra = np.array(core_spectra)\n",
    "    # pca for denoising\n",
    "    clean_spectra = denoise_pca(core_spectra)\n",
    "    pp_spectra, pp_wavenumbers = preprocess_spectra(clean_spectra, wavenumbers)\n",
    "    \n",
    "    spectral_image = np.zeros((ypixels, xpixels, pp_wavenumbers.shape[0])) \n",
    "    for (row,col), spectra in zip(core_indices, pp_spectra):\n",
    "        spectral_image[row, col, :] = spectra\n",
    "\n",
    "    # plt.imshow(spectral_image[:,:,365])\n",
    "    # plt.axis(\"off\")\n",
    "    # plt.show()\n",
    "    \n",
    "\n",
    "    core_patches = 0\n",
    "    for i,(row, col) in enumerate(core_indices):\n",
    "        \n",
    "        if row < half_patch or row >= ypixels - half_patch or col < half_patch or col >= xpixels - half_patch:\n",
    "            continue  # Skip out of bounds patches\n",
    "        spectral_patch = spectral_image[row-half_patch:row+half_patch+1, col-half_patch:col+half_patch+1, :]\n",
    "        # Skip patches that are < 85% filled with pixels\n",
    "        if np.count_nonzero(spectral_patch) / spectral_patch.size < 0.85:  \n",
    "            continue  \n",
    "         # Check if reached the limit for the core\n",
    "        if core_patches >= 3000:\n",
    "            continue\n",
    "        spectral_patches.append(spectral_patch)\n",
    "        labels.append(label[i])  \n",
    "        core_id.append(core_name)\n",
    "        core_patches += 1\n",
    "\n",
    "Y_train = np.array(labels, dtype=np.uint8)  # shape: (num_samples,)\n",
    "\n",
    "core_ids = np.array(core_id)\n",
    "# print(Counter(core_ids))\n",
    "\n",
    "X_train_chem_img = np.array(chem_img_patches, dtype=np.float32)  # shape: (num_samples, PATCH_SIZE, PATCH_SIZE)\n",
    "\n",
    "# --- Undersample ---\n",
    "target_count = 10000\n",
    "np.random.seed(42)\n",
    "\n",
    "# Get indices for class 0 and class 1\n",
    "idx_0 = np.where(Y_train == 0)[0]\n",
    "idx_1 = np.where(Y_train == 1)[0]\n",
    "\n",
    "# Sample 5,000 from each\n",
    "sampled_0 = np.random.choice(idx_0, size=target_count, replace=False)\n",
    "sampled_1 = np.random.choice(idx_1, size=target_count, replace=False)\n",
    "\n",
    "# Combine and shuffle\n",
    "final_train_idx = np.concatenate([sampled_0, sampled_1])\n",
    "\n",
    "# Apply to X_train and Y_train\n",
    "Y_train = Y_train[final_train_idx]\n",
    "X_train_spectra = np.array([spectral_patches[i] for i in final_train_idx], dtype=np.float32)\n",
    "core_ids = core_ids[final_train_idx]\n",
    "\n",
    "# X_train_spectra = np.array(spectral_patches, dtype=np.float32) # (num_samples, PATCH_SIZE, PATCH_SIZE, num_spectral_bands)\n"
   ]
  },
